{
  "name": "utools-tailwindcss",
  "version": "1.0.0",
  "description": "快速搜索 Tailwind CSS 文档",
  "scripts": {
    "watch": "vite build --watch",
    "build": "tsc --noEmit && vite build",
    "generate": "tailwindcss -i ./script/input.css -o ./script/output.css && node ./script/generator.js"
  },
  "author": "Trent0",
  "license": "MIT",
  "devDependencies": {
    "@types/css": "^0.0.33",
    "@types/node": "^20.3.1",
    "css": "^3.0.0",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "utools-api-types": "^3.0.0",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "utools-utils": "^1.1.10"
  }
}